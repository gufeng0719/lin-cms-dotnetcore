<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1</TargetFrameworks>
    <AssemblyName>VoVo.CAP.MySql</AssemblyName>
    <PackageTags>MySQL</PackageTags>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>VoVo.CAP.MySql.xml</DocumentationFile>
    <NoWarn>1701;1702;1705;CS1591</NoWarn>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.1</Version>
    <PackageId>VoVo.CAP.MySql</PackageId>
    <Authors>luoyunchong</Authors>
    <Company></Company>
    <Product>luoyunchong</Product>
    <RootNamespace>DotNetCore.CAP</RootNamespace>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://github.com/luoyunchong/lin-cms-dotnetcore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/luoyunchong/lin-cms-dotnetcore</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageReleaseNotes>VoVo.CAP.MySql为CAP增加FreeSql中的统一事务提交，删除了efcore 的事务。</PackageReleaseNotes>
    <Copyright></Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>0.0.1.1</AssemblyVersion>
    <FileVersion>0.0.1.1</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="VoVo.CAP.MySql.xml" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="VoVo.CAP.MySql.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.0.35" />
    <PackageReference Include="DotNetCore.CAP" Version="3.0.4-preview-103991021" />
    <PackageReference Include="FreeSql.DbContext" Version="1.4.0-preview20200411" />
    <PackageReference Include="MySqlConnector" Version="0.64.0" />
  </ItemGroup>

</Project>
